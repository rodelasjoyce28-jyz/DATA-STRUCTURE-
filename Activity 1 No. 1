#include <iostream>
using namespace std;

int main() {
    int i = 0;

  // Print numbers 1 to 10
    while (i <= 10) {
        cout << i << " ";
        i++;
    }
    cout << endl;

   // Print numbers 10 to 1
    i = 10;
    while (i >= 0) {
        cout << i << " ";
        i--;
    }

    return 0;
}



