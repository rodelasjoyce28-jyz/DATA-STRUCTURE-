#include <iostream>
using namespace std;

int main() {
    int num, sum = 0;

    cout << "Enter integers (0 to stop): ";
    cin >> num;

    while (num != 0) {
        if (num > 0) {
            sum += num;
        }
        cin >> num;
    }

    cout << "Sum of positive integers: " << sum << endl;

    return 0;
}
