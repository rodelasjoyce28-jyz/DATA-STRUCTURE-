#include <iostream>
#include <string>
using namespace std;

int main() {
  string fullName, courseYrSec, birthday, motto;
  
 
  cout << "Enter your Full Name: ";
  getline(cin, fullName);
  
  cout << "Enter your courseYrSec: ";
  getline(cin, courseYrSec);
  
  cout << "Enter your birthday: ";
  getline(cin, birthday);
  
  cout << "Enter your motto: ";
  getline(cin, motto);
  
  
  cout <<  "Hi! Im " << fullName << " of " <<  courseYrSec  << " Welcome to Data Structures & Algorithm!" << endl;
  cout << "My birthday is on " << birthday << " and my motto/motivation in life is \" " <<  motto  << "\"" << endl;
 
  

  return 0;
  }
